import random

random.randint(10,20)


# -100  ~ 100 까지 정수 난수 생성 양수는 pos 리스트에 음수는 neg 리스트에 저장, 0이 되면 종료

pos = []
neg = []

r = random.randint(-100,100)

while r != 0:
    if r > 0:
        pos.append(r)
    else:
        neg.append(r)
    r = random.randint(-100,100)
    
print("종료")
        
print(f"pos : {pos}\n")
print(f"neg : {neg}")


pos = []
neg = []

while r := random.randint(-100,100): # := (왈러스 연산자 - 대입연산자) - if / while 조건절. 값을 변수에 대입하고 반환
    if r > 0:
        pos.append(r)
    else:
        neg.append(r)
        
print("종료")

print(f"pos : {pos}\n")
print(f"neg : {neg}")





